{
    "python.pythonPath": ".tox/local/bin/python",
    "restructuredtext.sphinxBuildPath": "sphinx-build",
    "restructuredtext.confPath": "${workspaceFolder}",
    "restructuredtext.linter.extraArgs": [
        "--ignore D001"
    ],
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.mypyEnabled": true,
    "python.linting.pycodestyleEnabled": true,
    "python.linting.enabled": true,
    "python.linting.pycodestyleArgs": ["--ignore", "E501,E251,E701"],
    "python.testing.cwd": "${workspaceFolder}",
    "python.testing.unittestEnabled": false,
    "python.testing.nosetestsEnabled": false,
    "python.testing.pytestEnabled": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.coverage.*": true,
        "**/__pycache__" : true,
        "**/.pytest_cache" : true,
        "**/*.egg-info": true
    },
    "cSpell.words": [
        "Alignas",
        "Alignof",
        "Armin",
        "Buildkite",
        "Doctest",
        "Noreturn",
        "Pytest",
        "Pytest's",
        "Ronacher",
        "apidoc",
        "autofunction",
        "bitor",
        "chmod",
        "compl",
        "conftest",
        "datetime",
        "deque",
        "docstring",
        "docstrings",
        "doctests",
        "dsdl",
        "endblock",
        "fnmatch",
        "functools",
        "functors",
        "gentest",
        "getenv",
        "ifdef",
        "ifndef",
        "inout",
        "isfunction",
        "isroutine",
        "itertools",
        "kwlist",
        "macrofy",
        "managedsecretsbucket",
        "markupsafe",
        "mkdir",
        "mypy",
        "nnvg",
        "noexcept",
        "noindex",
        "noqa",
        "nosec",
        "nullptr",
        "ptest",
        "pycache",
        "pydsdl",
        "pyenv",
        "pypi",
        "pytest",
        "pyyaml",
        "rcfile",
        "reflexpr",
        "scotec",
        "serializable",
        "sonarcloud",
        "struct",
        "submodule",
        "uint",
        "virtualenv",
        "wchar",
        "xargs",
        "yamlfy"
    ],
    "cmake.configureOnOpen": false
}
