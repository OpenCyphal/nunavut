{
    "python.pythonPath": ".pyenv/bin/python",
    "restructuredtext.sphinxBuildPath": "sphinx-build",
    "restructuredtext.confPath": "${workspaceFolder}/src",
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.mypyEnabled": true,
    "python.linting.pep8Enabled": true,
    "python.linting.enabled": true,
    "python.linting.pep8Args": ["--ignore", "E501,E251,E701"],
    "cSpell.words": [
        "Codacy",
        "Pytest",
        "Pytest's",
        "autofunction",
        "chmod",
        "deque",
        "dsdl",
        "functors",
        "gentest",
        "ifdef",
        "ifndef",
        "isfunction",
        "isroutine",
        "itertools",
        "kwlist",
        "macrofy",
        "mkdir",
        "mypy",
        "nnvg",
        "noindex",
        "noqa",
        "nosec",
        "ptest",
        "pycache",
        "pydsdl",
        "pyenv",
        "pytest",
        "pyyaml",
        "scotec",
        "serializable",
        "struct",
        "submodule",
        "virtualenv",
        "xargs",
        "yamlfy"
    ],
    "python.testing.cwd": "${workspaceFolder}/test",
    "python.testing.unittestEnabled": false,
    "python.testing.nosetestsEnabled": false,
    "python.testing.pytestEnabled": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.coverage.*": true,
        "**/__pycache__" : true,
        "**/.pytest_cache" : true,
        "**/*.egg-info": true
    }
}